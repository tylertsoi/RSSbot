package de.vogella.rss.global;


public class GlobalRssText {

	  static String pubDate;
	  static String title;
	  static String guid;
	  
	  public GlobalRssText(String pubDate, String title, String guid) {
		    this.pubDate = pubDate;
		    this.title = title;
		    this.guid = guid;
		  }
	  
	  public static String getPubDate() {
		    return pubDate;
		  }
	  
	  public static String getTitle() {
		    return title;
		  }
	  
	  public static String getGuid() {
		    return guid;
		  }
	
}
